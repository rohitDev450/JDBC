<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">
	<bean class="com.jspider.SpringCore.entity.User" name="user1">
		<property name="id" value="1" />
		<property name="name" value="Rohit" />
		<property name="email" value="rohit@gmail.com" />
		<property name="password" value="Rohit@1234" />
	</bean>

	<bean class="com.jspider.SpringCore.entity.User" name="user2">
		<property name="id" value="2" />
		<property name="name" value="Mohit" />
		<property name="email" value="mohit@gmail.com" />
		<property name="password" value="Mohit@1234" />

	</bean>
	
	<bean class="com.jspider.SpringCore.entity.User" name="user3">
	 <constructor-arg name="id" value="1"></constructor-arg>
	 <constructor-arg name="name" value="Kartik"></constructor-arg>
	 <constructor-arg name="email" value="kartik@gmail.com"></constructor-arg>
	 <constructor-arg name="password" value="Kartik@134"></constructor-arg>
	
	</bean>
</beans>